  "blocked_keywords": [
    "violencia",
    "drogas",
    "alcohol",
    "armas",
    "sangre",
    "muerte",
    "suicidio",
    "autolesión",
    "bullying",
    "contraseña",
    "password",
    "dirección",
    "teléfono",
    "tarjeta",
    "dinero",
    "comprar",
    "pagar",
    "cita",
    "encuentro",
    "foto"
  ],
  "emergency_contacts": [
    {
      "name": "Papá",
      "action": "notify_parent"
    },
    {
      "name": "Mamá",
      "action": "notify_parent"
    }
  ]
}